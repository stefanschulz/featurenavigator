{
  "name": "the-loom/featurenavigator",
  "description": "Navigate products via a feature",
  "minimum-stability": "stable",
  "license": "AFL-3.0",
  "type": "prestashop-module",
  "authors": [
    {
      "name": "Stefan",
      "email": "schulz@the-loom.de"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "v3.41.1",
    "prestashop/php-dev-tools": "^4.3",
    "prestashop/autoindex": "v2.1.0"
  },
  "autoload": {
    "psr-4": {
      "PrestaShop\\Module\\FeatureNavigator\\": "src/"
    },
    "classmap": [
      "featurenavigator.php",
      "controllers/",
      "src/"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false
  },
  "scripts": {
    "php-cs-fixer": "php-cs-fixer --rules=@PSR1"
  }
}
